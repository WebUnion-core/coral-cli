    "name": "bona-storm",
    "version": "1.0.5",
    "description": "Bona-Storm",
    "main": "server.js",
    "scripts": {
        "dev": "set NODE_ENV=development&&set IF_DEBUG=1&&webpack-dev-server",
        "build-dev": "set NODE_ENV=development&&webpack",
        "build-prod": "set NODE_ENV=production&&webpack",
        "start-dev-server": "set NODE_ENV=development&&node server.js",
        "start-prod-server": "set NODE_ENV=production&&node server.js",
        "watch": "nodemon --exec npm run start-dev-server",
        "build-server": "babel controllers -d build",
        "build-sprite": "webpack&&node ./bin/reset-sprite.js"
    },
